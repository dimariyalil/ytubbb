{
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "framework": "vite",
  "functions": {
    "api/*.ts": {
      "runtime": "@vercel/node@3"
    }
  },
  "env": {
    "YOUTUBE_API_KEY": "@youtube-api-key",
    "CLAUDE_API_KEY": "@claude-api-key",
    "MONGODB_URI": "@mongodb-uri",
    "REDIS_URL": "@redis-url"
  }
}